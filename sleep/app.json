{
	"app": {
		"name": "Sleep",
		"slug": "sleep",
		"entry_name": "night",
		"description": "sleep tracker",
		"private": false,
		"icon": "ðŸ˜€",
		"color": "#0cc4e9",
		"storageType": "",
		"storage": "",
		"charts_placement": "",
		"entries_name": "nights"
	},
	"temporal": {
		"times": "yes",
		"ranges": ""
	},
	"entry": [
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal",
				"deletable"
			],
			"settings": {
				"expression": "weekday(date, 'shortname')"
			},
			"name": "weekday"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"type": "date",
				"default": "[$today]",
				"formatting": [
					"b"
				]
			},
			"name": "date"
		},
		{
			"type": "property",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"prefix": " at ",
				"type": "time",
				"default": "[time($startup, 'hours')]",
				"format": "[time(time, 'minutes')]"
			},
			"name": "time"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"prefix": "end:",
				"type": "time",
				"default": "08:00"
			},
			"name": "end"
		},
		{
			"type": "expression",
			"scope": "entry",
			"flags": [
				"temporal"
			],
			"settings": {
				"visible": "false",
				"expression": "date & if(time, 'T' & time)"
			},
			"name": "date_time"
		},
		{
			"type": "expression",
			"scope": "entry",
			"settings": {
				"expression": "if(duration>5*hours(),'good', 'bad')"
			},
			"name": "quality"
		},
		{
			"type": "property",
			"scope": "entry",
			"settings": {
				"type": "text",
				"appearance": "line",
				"multiline": true
			},
			"name": "notes"
		}
	],
	"root": [
		{
			"type": "action",
			"scope": "root",
			"settings": {
				"label": "Add night",
				"target": "add(entry)",
				"target_preset": "new",
				"appearance": "stretch",
				"set_pairs": []
			}
		},
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"prefix": "good:",
				"expression": "count(quality=='good')"
			}
		},
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"prefix": "bad:",
				"expression": "count(quality='bad')"
			}
		},
		{
			"type": "expression",
			"scope": "root",
			"settings": {
				"prefix": "avg:",
				"expression": "duration(average(duration))"
			}
		}
	],
	"charts": []
}